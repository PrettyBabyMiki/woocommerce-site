{
	"root": "plugins/woocommerce/legacy",
	"sourceRoot": "plugins/woocommerce/legacy",
	"type": "application",
	"implicitDependencies": [],
	"targets": {
		"build": {
			"executor": "@nrwl/workspace:run-commands",
			"options": {
				"commands": [
					{
						"command": "pnpm dlx grunt assets",
						"forwardAllArgs": false
					}
				],
				"cwd": "plugins/woocommerce/legacy"
			},
			"outputs": [
				"plugins/woocommerce/assets/css",
				"plugins/woocommerce/assets/js"
			]
		},
		"lint": {
			"executor": "@nrwl/workspace:run-commands",
			"options": {
				"commands": [
					{
						"command": "pnpm dlx grunt eslint",
						"forwardAllArgs": false
					},
					{
						"command": "pnpm dlx grunt stylelint",
						"forwardAllArgs": false
					}
				],
				"cwd": "plugins/woocommerce/legacy"
			}
		}
	}
}
