{
  "extends": "@nrwl/workspace/presets/npm.json",
  "npmScope": "woocommerce-monorepo",
  "tasksRunnerOptions": {
    "default": {
      "runner": "@nrwl/workspace/tasks-runners/default",
      "options": {
        "cacheableOperations": [
          "build",
          "test",
          "lint",
          "package",
          "prepare"
        ]
      }
    }
  },
  "targetDependencies": {
    "build": [
      {
        "target": "build",
        "projects": "dependencies"
      }
    ],
    "prepare": [
      {
        "target": "prepare",
        "projects": "dependencies"
      }
    ],
    "package": [
      {
        "target": "package",
        "projects": "dependencies"
      }
    ]
  },
  "woocommerce": {
    "implicitDependencies": [ "woocommerce-legacy-assets" ]
  },
  "woocommerce-legacy-assets": {
    "implicitDependencies": []
  },
  "affected": {
    "defaultBase": "trunk"
  }
}
